{
  "name": "@typed-code/schemats",
  "version": "4.0.0",
  "description": "Generate typescript interface definitions from (Postgres or MySql) SQL database schema",
  "keywords": [
    "postgres",
    "mysql",
    "schema",
    "typescript",
    "sql"
  ],
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json --fix",
    "build": "tsc",
    "pretest": "npm run lint && npm run build",
    "test": "jest",
    "prepublish": "npm run build",
    "clean": "del-cli node_modules **/*.js"
  },
  "bin": {
    "schemats": "dist/bin/schemats.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typed-code/schemats.git"
  },
  "bugs": {
    "url": "https://github.com/typed-code/schemats/issues"
  },
  "author": "Mengxuan Xia <xiamx2004@gmail.com>",
  "contributors": [
    "Arnaud Benhamdine <arnaud.benhamdine@gmail.com>",
    "zigomir <zigomir@gmail.com>",
    "Mark Crisp <macr1324@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/diff": "^4.0.2",
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.136",
    "@types/mysql": "2.15.5",
    "@types/mz": "^0.0.32",
    "@types/node": "^11.13.19",
    "@types/yargs": "^12.0.12",
    "del-cli": "^1.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "dependencies": {
    "diff": "^4.0.1",
    "lodash": "^4.17.15",
    "mysql": "^2.17.1",
    "mz": "^2.7.0",
    "pg-promise": "^8.7.5",
    "typescript": "^3.5.3",
    "typescript-formatter": "^7.2.2",
    "yargs": "^13.3.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.[j|t]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
