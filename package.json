{
  "name": "@typed-code/schemats",
  "version": "4.0.0",
  "description": "Generate typescript interface definitions from (postgres) SQL database schema",
  "keywords": [
    "postgres",
    "schema",
    "typescript",
    "sql"
  ],
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json --fix",
    "build": "tsc",
    "pretest": "npm run lint && npm run build",
    "test": "mocha",
    "prepublish": "npm run build",
    "clean": "del-cli node_modules **/*.js",
    "coverage": "npm run lint && npm run build && nyc mocha",
    "coverage:ci": "npm run lint && npm run build && nyc mocha && nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "schemats": "dist/bin/schemats.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typed-code/schemats.git"
  },
  "bugs": {
    "url": "https://github.com/typed-code/schemats/issues"
  },
  "author": "Mengxuan Xia <xiamx2004@gmail.com>",
  "contributors": [
    "Arnaud Benhamdine <arnaud.benhamdine@gmail.com>",
    "zigomir <zigomir@gmail.com>",
    "Mark Crisp <macr1324@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/diff": "^4.0.1",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/mysql": "2.15.5",
    "@types/mz": "^0.0.32",
    "@types/node": "^11.11.3",
    "@types/power-assert": "^1.5.0",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.0.10",
    "@types/yargs": "^12.0.9",
    "coveralls": "^3.0.3",
    "del-cli": "^1.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "power-assert": "^1.6.1",
    "prettier": "^1.16.4",
    "proxyquire": "^2.1.0",
    "sinon": "^7.2.7",
    "source-map-support": "^0.5.11",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "diff": "^4.0.1",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "mz": "^2.7.0",
    "pg-promise": "^8.6.4",
    "typescript": "^3.3.3333",
    "typescript-formatter": "^7.2.2",
    "yargs": "^13.2.2"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
